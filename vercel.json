{
  "version": 2,
  "framework": "angular",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/aurora-e-shop/browser",
        "installCommand": "npm install --legacy-peer-deps"
      }
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "ng build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
